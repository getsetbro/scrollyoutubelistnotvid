{
  "manifest_version": 2,
  "name": "Scroll YT vid list not vid",
  "version": "2.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Scroll YT vid list not vid"
  },
  "permissions": [
    "activeTab","http://www.youtube.com/watch*"
  ]
}
